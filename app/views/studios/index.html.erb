<h1>Studios</h1>
<%  @studios.each do |studio| %>
  <div id="studio-<%= studio.id %>">
    <h3> <%= studio.name %></h3>
    <p>Located in: <%= studio.location %> </p>
    <h4>Movies Produced:</h4>
    <% studio.movies.each do |movie| %>
      <p><%= movie.title %></p>
    <% end %>
  </div>
<% end %>
